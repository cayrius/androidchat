<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidchat.client">
    <uses-permission android:name="android.permission.ACCESS_LOCATION" /> 
    <uses-permission android:name="android.permission.ACCESS_GPS" />
    <uses-permission id="android.permission.ACCESS_LOCATION" /> 
    <uses-permission id="android.permission.ACCESS_GPS" />
    <application android:icon="@drawable/icon">

        <activity android:name=".ActivityAndroidChatMain" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".ActivityChatChannel"></activity>      
        <activity android:name=".AndroidChatMap"></activity>
        
        <activity android:name=".ChannelGrid"></activity>
        <activity android:name=".ActivityOptions"></activity>
        
        
        <service android:name=".ServiceIRCService" android:label="IRC Connectivity Service">

        </service>
         
    </application>
</manifest> 